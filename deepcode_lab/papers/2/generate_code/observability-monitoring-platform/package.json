{
  "name": "observability-monitoring-platform",
  "version": "1.0.0",
  "description": "Comprehensive Frontend Demo System - Integrated multi-dimensional observability visualization (metrics, traces, logs) with seamless cross-module navigation for rapid fault localization",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "pinia": "^2.1.3",
    "vue-router": "^4.2.4",
    "element-plus": "^2.4.0",
    "echarts": "^5.4.2",
    "@antv/g6": "^5.0.0",
    "vue-virtual-scroller": "^1.0.10",
    "sass": "^1.66.1",
    "@iconify/vue": "^4.1.1",
    "gsap": "^3.12.2",
    "dayjs": "^1.11.10",
    "uuid": "^9.0.0",
    "mockjs": "^1.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/test-utils": "^2.4.1",
    "vitest": "^0.34.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.0.2",
    "@types/node": "^20.5.1",
    "vue-tsc": "^1.8.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
